{
  "name": "viva",
  "version": "1.0.0",
  "description": "viva test",
  "main": "index.js",
  "scripts": {
    "lint": "eslint test src",
    "clean": "rimraf build && mkdir build",
    "cleanandtest": "concurrently -r \"git clean -d -f\" \"npm run test\"",
    "version": "concurrently -r \"npm run build\" \"git add -A build\"",
    "postversion": "concurrently -r \"git push\" \"git push --tags\"",
    "test": "babel-tape-runner test/**/*.js | tap-spec",
    "testcov": "nyc --reporter=lcov --reporter=text-lcov babel-tape-runner test/*.js | tap-spec",
    "prebuild": "npm run clean",
    "build": "browserify src/index.js -d -t [ babelify --sourceMapRelative . --presets [ es2015 react ] ] -p [ minifyify --map app.map --output build/app.map] -o build/app.js",
    "watch": "watchify src/index.js -d -t [ babelify --sourceMapRelative . --presets [ es2015 react ] ] -p [ minifyify --map app.map --output build/app.map] -o build/app.js",
    "dev": "webpack --display-error-details --watch",
    "start": "node server.js"
  },
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-core": "^6.0.20",
    "babel-eslint": "^4.1.3",
    "babel-loader": "^6.0.1",
    "babel-plugin-transform-runtime": "^6.7.5",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.0.15",
    "babel-runtime": "^6.6.1",
    "babel-tape-runner": "^2.0.1",
    "babelify": "^7.3.0",
    "blue-tape": "^0.2.0",
    "browserify": "^13.0.0",
    "eslint": "^2.4.0",
    "eslint-config-airbnb": "^6.1.0",
    "eslint-plugin-react": "^4.2.3",
    "minifyify": "^7.3.2",
    "mkdirp": "^0.5.1",
    "nyc": "^6.4.0",
    "tap-spec": "^4.1.1",
    "tape": "^4.5.1",
    "watchify": "^3.7.0"
  },
  "author": "Andy Willis",
  "license": "ISC"
}
