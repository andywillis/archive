{
  "extends": ["airbnb-base", "standard-preact"],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 8,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "env": {
    "jest": true,
    "browser": true,
    "node": true
  },
  "rules": {
    "class-methods-use-this": 0,
    "no-underscore-dangle": 0,
    "no-plusplus": 0,
    "default-case": 0,
    "func-names": 0,
    "consistent-return": 0,
    "space-before-function-paren": [2, { "anonymous": "always", "named": "never" }],
    "no-console": 0,
    "import/extensions": 0,
    "array-bracket-spacing": 0,
    "comma-dangle": 0,
    "padded-blocks": 0,
    "arrow-body-style": 0,
    "no-shadow": 0,
    "no-param-reassign": 0,
    "no-tabs": 0
  }
}
